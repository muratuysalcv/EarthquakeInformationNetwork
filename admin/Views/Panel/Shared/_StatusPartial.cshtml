@{ var response = ViewBag.RoleResponse;
    if (response != null)
    {
        if (response.Message != null && response.Message.Count > 0)
        {
            <script>var array = []</script>
            for (int i = 0; i < response.Message.Count; i++)
            {
                <script>array.push("@response.Message[i]")</script>
            }
            if (response.Status)
            {

                <script>Notification.push("success", ArrayToString(array))</script>
            }
            else
            {
                <script>Notification.push("error", ArrayToString(array))</script>
            }

        }
    }
}